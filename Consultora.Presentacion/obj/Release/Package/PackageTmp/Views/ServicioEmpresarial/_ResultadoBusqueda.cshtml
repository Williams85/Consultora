@model List<Consultora.Entidad.ServicioEmpresarialCompetenciaEntidad>
    @{
        ViewBag.Title = "_ResultadoBusqueda";
    }
    @if (Model != null && Model.Count > 0)
    {
        <h5>Información de Consultores</h5>

        <table>
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Cargo</th>
                    <th>Senority</th>
                    @*<th>Disponibilidad</th>*@
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var entidad in Model)
                {
                    <tr>
                        <td>@entidad.Consultor.NombreCompleto</td>
                        <td>
                            @entidad.Consultor.Competencia.Nom_Competencia
                        </td>
                        <td>@entidad.Consultor.NivelCompetencia.Nom_Nivel_Competencia</td>
                        @*<td class="center"> 1</td>*@
                        <td class="center">
                            <button type="button" data-id="@entidad.Cod_Servicio_Empresarial_Competencias" class="btn btn-danger btn-eliminar-consultor">Eliminar</button>
                        </td>
                    </tr>
                }

            </tbody>
        </table>

        <div class="lista-botones">
            <button id="buscar-consultor" class="btn btn-success">Agregar Consultor</button>
            <button id="guardar-datos" class="btn btn-primary">Guardar Datos</button>
</div>
    }

