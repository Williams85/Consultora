@model Consultora.Entidad.ConsultorEntidad
    @{
        ViewBag.Title = "_ResultadoDetalleConsultorModal";
    }

@if (Model != null)
{
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-content nopadding">
                    <table class="table table-bordered table-striped">
                        <tbody>

                            <tr class="odd gradeX">
                                <td>Consultor:</td>
                                <td>@Model.NombreCompleto</td>
                            </tr>

                            <tr class="odd gradeX">
                                <td>Cargo:</td>
                                <td>@Model.Competencia.Nom_Competencia</td>
                            </tr>

                            <tr class="odd gradeX">
                                <td>Seniority:</td>
                                <td>@Model.NivelCompetencia.Nom_Nivel_Competencia</td>
                            </tr>

                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>

}

