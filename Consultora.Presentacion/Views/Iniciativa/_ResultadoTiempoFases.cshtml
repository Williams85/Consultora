@model List<Consultora.Entidad.IniciativaFaseEntidad>
    @{
        ViewBag.Title = "_ResultadoTiempoFases";
    }

    @if (Model != null && Model.Count > 0)
    {
        <h6>Resultados por Fases</h6>
        <table>
            <thead>
                <tr>
                    <th>Fase</th>
                    <th>Horas Estimadas</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var entidad in Model)
                {
                    <tr>
                        <td>
                            @entidad.Fase.Nom_Fase
                        </td>
                        <td class="center">
                            @entidad.Horas
                        </td>
                    </tr>
                }

            </tbody>
        </table>
    }

